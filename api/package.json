{
    "name": "octopus",
    "version": "1.0.0",
    "description": "A new way to publish your scientific work that's fast, free and fair.",
    "author": "0-3 Years Team",
    "keywords": [],
    "license": "GPL-3.0-only",
    "repository": {
        "type": "git",
        "url": "https://github.com/JiscSD/octopus/tree/main/api"
    },
    "engines": {
        "node": "18.x"
    },
    "prisma": {
        "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts"
    },
    "scripts": {
        "prepare": "cd .. && husky install",
        "start:local": "nodemon --config nodemon.json",
        "seed:local": "STAGE=local npx prisma migrate reset --force",
        "test:local": "STAGE=local jest --runInBand --testTimeout=60000",
        "test:watch": "npm run test:local -- --watch",
        "reindex": "ts-node prisma/reindex.ts",
        "createVersionedDOIs": "ts-node -r tsconfig-paths/register prisma/createVersionedDOIs.ts",
        "updateLinksStatus": "ts-node -r tsconfig-paths/register prisma/updateLinksStatus.ts",
        "insertDummyEvent": "ts-node -r tsconfig-paths/register prisma/insertDummyEvent.ts",
        "deleteHighLevelTopicBookmarks": "ts-node -r tsconfig-paths/register prisma/deleteHighLevelTopicBookmarks.ts",
        "format:check": "npx prettier --check src/",
        "format:write": "npx prettier --write src/",
        "lint:check": "eslint src/",
        "lint:fix": "eslint --fix src/"
    },
    "dependencies": {
        "@middy/core": "^4.7.0",
        "@middy/do-not-wait-for-empty-event-loop": "^4.7.0",
        "@middy/http-json-body-parser": "^4.7.0",
        "@opensearch-project/opensearch": "^2.5.0",
        "@paralleldrive/cuid2": "^2.2.2",
        "@prisma/client": "^4.16.2",
        "@sparticuz/chromium": "^114.0.0",
        "ajv": "^8.12.0",
        "ajv-formats": "^2.1.1",
        "axios": "^1.6.5",
        "cheerio": "^1.0.0-rc.12",
        "copy-webpack-plugin": "^11.0.0",
        "html-to-text": "^8.2.1",
        "jsonwebtoken": "^9.0.2",
        "luxon": "^3.4.3",
        "nodemailer": "^6.9.9",
        "puppeteer-core": "^20.9.0",
        "supertest": "^6.3.3"
    },
    "devDependencies": {
        "@aws-sdk/client-s3": "^3.413.0",
        "@aws-sdk/client-ses": "^3.413.0",
        "@aws-sdk/client-sqs": "^3.413.0",
        "@types/aws-lambda": "^8.10.121",
        "@types/cheerio": "^0.22.32",
        "@types/jest": "^29.5.4",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/luxon": "^3.3.2",
        "@types/node": "^18.17.15",
        "@types/nodemailer": "^6.4.10",
        "@types/serverless": "^3.12.15",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "aws-lambda": "^1.0.7",
        "eslint": "^8.49.0",
        "eslint-config-airbnb-typescript": "^16.2.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "lint-staged": "^13.3.0",
        "nodemon": "^2.0.22",
        "prettier": "^2.8.8",
        "puppeteer": "^20.9.0",
        "serverless-domain-manager": "^7.1.2",
        "serverless-offline": "^12.0.4",
        "serverless-offline-ssm": "^6.2.0",
        "serverless-prune-plugin": "^2.0.2",
        "serverless-webpack": "^5.13.0",
        "serverless-webpack-prisma": "^1.2.0",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.4.4",
        "tsconfig-paths-webpack-plugin": "^4.1.0",
        "typescript": "^5.2.2",
        "webpack": "^5.88.2",
        "webpack-node-externals": "^3.0.0"
    },
    "overrides": {
        "fast-xml-parser": "^4.2.5",
        "http-cache-semantics": "^4.1.1",
        "node-fetch": "^2.6.12"
    }
}
