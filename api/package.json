        "test:watch": "npm run test:local -- --watch",
        "reindex": "ts-node prisma/reindex.ts",
        "format:check": "npx prettier --check src/",
        "format:write": "npx prettier --write src/",
        "lint:check": "eslint src/",
        "lint:fix": "eslint --fix src/"
