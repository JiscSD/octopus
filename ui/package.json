{
    "name": "ui",
    "version": "1.0.0",
    "description": "Octopus is a new way to register research. It is the place to publish the version of record, enabling peer review and quality assessment and allowing the academic community to build upon the latest work.",
    "author": "0-3 Years Team",
    "keywords": [],
    "license": "GPL-3.0-only",
    "repository": {
        "type": "git",
        "url": "https://github.com/JiscSD/octopus/tree/main/ui"
    },
    "engines": {
        "node": "14.x"
    },
    "scripts": {
        "dev": "concurrently \"NEXT_PUBLIC_VERCEL_GIT_COMMIT_REF=local next dev -p 3000\" \"local-ssl-proxy --source 3001 --target 3000\"",
        "build": "next build",
        "start": "next start -p 3000",
        "start:int": "NEXT_PUBLIC_VERCEL_GIT_COMMIT_REF=int next start -p 3000",
        "test:e2e": "npx playwright test --project=chromium && npx playwright test --project=firefox",
        "lint": "next lint",
        "format:check": "npx prettier --check src/",
        "format:write": "npx prettier --write src/",
        "lighthouse": "lhci autorun"
    },
